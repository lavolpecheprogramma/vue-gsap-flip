{
  "name": "@vue-gsap-flip/reduced-motion",
  "type": "module",
  "version": "1.0.0",
  "description": "Reduced motion support for vue-gsap-flip animations",
  "author": "Alberto Parziale <parziale.alberto@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lavolpecheprogramma/vue-gsap-flip.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "keywords": [
    "vue",
    "gsap",
    "flip"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "type-check": "vue-tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "gsap": "^3.13.0",
    "vue": "^3.0.0"
  },
  "dependencies": {
    "@vue-gsap-flip/core": "1.0.0"
  }
}
