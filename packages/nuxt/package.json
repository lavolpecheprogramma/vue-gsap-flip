{
  "name": "@vue-gsap-flip/nuxt",
  "type": "module",
  "version": "1.0.0",
  "description": "Nuxt 3 module for vue-gsap-flip with auto-registration",
  "author": "Alberto Parziale <parziale.alberto@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lavolpecheprogramma/vue-gsap-flip.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "keywords": [
    "vue",
    "gsap",
    "flip",
    "nuxt",
    "nuxt-module"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "npm run dev:prepare && nuxt-module-build build",
    "dev": "npm run dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare",
    "build": "npm run lint && npm run prepack",
    "lint": "eslint .",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^4.0.1",
    "@vue-gsap-flip/core": "^1.2.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.6.2",
    "@nuxt/eslint-config": "^1.7.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.0.1",
    "@types/node": "latest",
    "eslint": "^9.32.0",
    "nuxt": "^4.0.1",
    "typescript": "~5.8.3",
    "vue-tsc": "^3.0.4"
  }
}
